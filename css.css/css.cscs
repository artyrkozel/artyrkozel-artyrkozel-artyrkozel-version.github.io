.price {
  display: flex;
  align-items: flex-end;
  justify-content: center;
}
.price p {
  font-size: 60px;
  position: relative;
  line-height: 0.7;
  margin: 0 10px 0 10px;
  font-weight: 800;
  color: #383838;
}
.price p::after {
  content: "";
  width: 12px;
  height: 12px;
  border-radius: 3px;
  background-color: #484848;
  transform: rotate(45deg);
  display: inline-block;
  margin-left: 6px;
}
.price span {
  font-size: 16px;
  color: #383838;
  position: absolute;
  top: 0;
  right: -3px;
}
.price i {
  color: #8a8a8a;
}
.price sup {
  font-weight: normal;
  font-size: 12px;
  color: #8a8a8a;
}

.line-price::before, .line-price::after {
  content: "";
  display: block;
  width: 90%;
  height: 2px;
  background-color: #ebebeb;
  margin: 25px auto;
}
.line-price ul li {
  margin-top: 25px;
  font-size: 14px;
  font-weight: 500;
  color: #7a7a7a;
}
.line-price ul li:first-child {
  margin: 0;
}
.line-price button {
  font-size: 13px;
  color: #e4e4e4;
  margin-bottom: 25px;
  background-color: #00a99d;
}

.wr-purchase {
  background-color: #8a8a8a;
}
.wr-purchase .container {
  display: flex;
  justify-content: space-between;
  padding: 73px 0;
}
.wr-purchase .btn-contacts a {
  background-color: #00a99d;
  color: #e4e4e4;
  transition: 0.6s;
}
.wr-purchase p {
  font-size: 26px;
  color: #383838;
  font-weight: 800;
}
.wr-purchase p i {
  color: #00a99d;
}

.wr-footer {
  background-color: #383838;
  padding-top: 50px;
}
.wr-footer .container {
  text-align: center;
}
.wr-footer .social {
  width: 620px;
  display: flex;
  justify-content: space-between;
  margin: 0 auto;
  margin-top: 65px;
  padding-bottom: 75px;
}
.wr-footer .social-icon {
  width: 45px;
  height: 40px;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 2px solid #606060;
  border-radius: 5px;
  transition: 0.6s;
}
.wr-footer .social-icon:hover {
  -webkit-transform: translateY(-7px);
  transform: translateY(-7px);
  box-shadow: 1px 1px #f8f8f8, 2px 2px #f8f8f8, 3px 3px #f8f8f8, 4px 4px;
}
.wr-footer .social-icon i {
  font-size: 17px;
  color: #e4e4e4;
}
.wr-footer .item-icon {
  position: relative;
}
.wr-footer .item-icon:hover .top-icon {
  background-color: #000;
}
.wr-footer .item-icon:hover .bottom-icon {
  background-color: #00a99d;
}
.wr-footer .item-icon .top-icon {
  width: 40px;
  height: 50px;
  display: block;
  background-color: #00a99d;
  clip-path: polygon(0 0, 100% 0, 100% 59%, 50% 100%, 0 59%);
  border-radius: 3px;
  position: absolute;
  top: 20px;
  transition: 0.6s;
}
.wr-footer .item-icon .bottom-icon {
  width: 40px;
  height: 50px;
  display: block;
  background-color: #000;
  clip-path: polygon(0 0, 100% 0, 100% 59%, 50% 100%, 0 59%);
  border-radius: 3px;
  position: absolute;
  transition: 0.6s;
}
.wr-footer .item-icon .bottom-icon img {
  margin-top: 10px;
}

.footer-top {
  border-radius: 5px;
}

.footer-middle {
  background-image: url("../media/map.jpg");
  background-repeat: no-repeat;
  padding: 60px 0;
}
.footer-middle .wr-form {
  background-color: #f8f9f9;
  width: 370px;
  padding: 20px;
  border-radius: 5px;
  border: 1px solid #dedede;
}
.footer-middle form {
  margin: 0 auto;
}
.footer-middle input {
  padding: 15px;
  border-radius: 5px;
  border: 1px solid #dedede;
}
.footer-middle input[type=text], .footer-middle input[type=email], .footer-middle textarea {
  font-style: italic;
  width: 330px;
  margin-bottom: 25px;
  font-size: 12px;
}
.footer-middle textarea {
  border: none;
  padding: 15px;
}
.footer-middle .dws-btn {
  width: 330px;
  background-color: #00a99d;
  color: #dedede;
  text-transform: uppercase;
  border: none;
  font-weight: 800;
  font-size: 13px;
  transition: 0.6s;
}
.footer-middle .dws-btn:hover {
  -webkit-transform: scale(0.9);
  -ms-transform: scale(0.9);
  transform: scale(0.9);
}

.footer-bottom {
  background-color: #383838;
  padding-top: 55px;
  padding-bottom: 30px;
}
.footer-bottom p {
  font-weight: 800;
  color: #cccccc;
}
.footer-bottom .icon-sec {
  position: relative;
  left: 50%;
  transform: translate(-50%, -150%);
}

