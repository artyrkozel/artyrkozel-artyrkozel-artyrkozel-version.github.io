.header_burger {
  display: none;
  position: absolute;
  right: 40px;
  width: 30px;
  height: 20px;
}
.header_burger span {
  position: absolute;
  left: 0;
  top: 9px;
  width: 100%;
  height: 2px;
  background-color: #000;
}
.header_burger::before, .header_burger::after {
  content: "";
  position: absolute;
  right: 0;
  width: 100%;
  height: 2px;
  background-color: #000;
}
.header_burger::before {
  top: 0;
}
.header_burger::after {
  bottom: 0;
}

.show {
  display: block;
}

@media all and (max-width: 1050px) {
  .header .nav-bar {
    display: none;
    position: absolute;
    right: 40px;
    top: 75px;
    background-color: #000;
    top: 100;
    opacity: 0.7;
  }
  .header .nav-bar .seach-icon {
    display: none;
  }
  .header .nav-bar a {
    color: #fff;
  }

  .header_burger {
    display: block;
  }
}
.header .nav-bar .show {
  display: block;
}

