  align-items: center;
  justify-content: center;
  border-radius: 10px;
  transform: rotate(45deg);
  margin-top: 50px;
  transition: 0.6s;
}
.info-card .wr-back .card-iocn:hover {
  z-index: 10;
  background-color: #383838;
  color: #fff;
}
.info-card .wr-back .card-iocn:hover i {
