.card {
  width: 310px;
  padding: 2px;
  background-color: #ffffff;
  position: relative;
  overflow: hidden;
  border-radius: 5px;
  padding-bottom: 10px;
  cursor: pointer;
}
.card::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  display: block;
  width: 100%;
  height: 100%;
  background-color: #1ca399;
  z-index: 2;
  transition: 0.5s all;
  opacity: 0;
}
.card:hover::before {
  opacity: 0.7;
}
.card img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  position: absolute;
  top: 0;
  left: 0;
}
.card .info-card {
  position: relative;
  z-index: 3;
  color: #ffffff;
  opacity: 0;
  transform: translateY(30px);
  transition: 0.5s all;
}
.card .info-card h4 {
  margin-top: 40px;
}
.card .info-card p {
  margin-top: 10px;
  color: #e4e4e4;
}
.card:hover .info-card {
  opacity: 1;
  transform: translateY(0px);
}

.info-card .wr-back {
  display: flex;
  justify-content: center;
}
.info-card .wr-back i {
  transform: rotate(-45deg);
